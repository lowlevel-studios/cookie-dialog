<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lowlevel.cookiedialog.sample"
    android:versionCode="1"
    android:versionName="1.0">

    <!--
        These permissions are used to know if an user is located in a EU country.
        They are not required for devices with a SIM card.
        There is no need to add all of them.
    -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light">
        
        <activity
            android:name="com.lowlevel.cookiedialog.sample.MainActivity">
            
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
        
	</application>
    
</manifest>